@model app.Models.MailData
@{
    ViewData["Title"] = "Inicio GCGC";
}

<div class="container">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                <!-- small box -->
                <div class="small-box bg-aqua">
                    <a href="@Url.Action("Index", "Presupuestos")" class="small-box bg-aqua">
                        <div class="inner">
                            <h3>Presupuestos</h3>

                            <p><h5>Envío Masivo de Presupuestos</h5></p>
                        </div>
