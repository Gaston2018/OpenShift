                        </div>
                    </a>
                        <a href="@Url.Action("Index", "Presupuestos")" class="small-box-footer">
                            Acceder <i class="fa fa-arrow-circle-right"></i>
                        </a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
                <!-- small box -->
                <div class="small-box bg-green">
                    <a href="@Url.Action("Index", "Renovaciones")" class="small-box bg-green">
                        <div class="inner">
                            <h3>Renovaciones</h3>

                            <p><h5>Envío Masivo de Renovaciones</h5></p>
                        </div>
                        <div class="icon">
                            <i class="fa fa-file-excel-o" aria-hidden="true"></i>
                        </div>
                    </a>
                        <a href="@Url.Action("Index", "Renovaciones")" class="small-box-footer">
                            Acceder <i class="fa fa-arrow-circle-right"></i>
                        </a>
                </div>
            </div>
            <!-- ./col -->
        </div>
